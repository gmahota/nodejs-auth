{
  "name": "nodejs-typescript-auth",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev 'src/index.ts'",
    "test": "yarn test:unit && yarn test:functional",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": "https://github.com/gmahota/nodejs-typescript-auth.git",
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Guimar√£es Mahota <guimaraesmahota@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gmahota/nodejs-typescript-auth/issues"
  },
  "homepage": "https://github.com/gmahota/nodejs-typescript-auth#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.10",
    "@types/jest": "^26.0.20",
    "@types/module-alias": "^2.0.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "@types/supertest": "^2.0.10",
    "axios": "^0.21.1"
  }
}
